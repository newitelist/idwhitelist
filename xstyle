    end 
end

end
end
end)
end
end)


llplayer:NewButton("Instantrespawn[On]", "Info", function()
    _G.Toggle = true
        while _G.Toggle do
            local args = {
                [1] = {
                    [1] = "Respawn"
                }
            }
            wait(1.1)
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
            for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if v.Name == "ForceField" then
                    v:Destroy()
                end
            end
        end
end)
llplayer:NewButton("Instantrespawn [Off]", "Info", function()
    _G.Toggle = false
        while _G.Toggle do
            local args = {
                [1] = {
                    [1] = "Respawn"
                }
            }
            wait(1.1)
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
            for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if v.Name == "ForceField" then
                    v:Destroy()
                end
            end
        end
    end)
