    _G.Toggle = true
        while _G.Toggle do
            local args = {
                [1] = {
                    [1] = "Respawn"
                }
            }
            wait(0.2)
            for _,v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
                if v.Name == "ForceField"  then
                    v:Destroy()
                end
            end
        end
        end)
llplayer:NewButton("NoShield[Off]", "Info", function()
        _G.Toggle = false
        while _G.Toggle do
            local args = {
                [1] = {
                    [1] = "Respawn"
                }
            }
            wait(0.2)
