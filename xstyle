da = state
while da do
game:GetService("RunService").RenderStepped:wait()
pcall (function()
    game:GetService("RunService").RenderStepped:wait()
            if game.Players:FindFirstChild(PlayerChosen).Character.Humanoid.Health ~= 0 then
if not game.Players:FindFirstChild(PlayerChosen).Character:FindFirstChild("SafeZoneShield") then
    
local z = game.Players.LocalPlayer.Character.HumanoidRootPart.Position.z

local z4 = game.Players:FindFirstChild(PlayerChosen).Character.HumanoidRootPart.Position.z
local y4 = game.Players:FindFirstChild(PlayerChosen).Character.HumanoidRootPart.Position.y
local x4 = game.Players:FindFirstChild(PlayerChosen).Character.HumanoidRootPart.Position.x
local args = {[1] = {[1] = "Skill_SpherePunch",[2] = Vector3.new(x, 9997999999999, z)}}
game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
for i, v in ipairs (game:GetService("Workspace").Storage:GetChildren()) do
    if v.Name == "EnergySphere9" then
        v.CFrame = CFrame.new(x4, y4+1, z4)


    end 
end
end
end
end)
end
end)


