px2 = state
while px2 do
task.wait()
pcall(function()
                local Name = game.Players[PlayerChosen].Character.HumanoidRootPart
    
                local function fspamm()
                    local Remote = game.ReplicatedStorage['RemoteEvent']
                    local Arguments = {
                        [1] = {
                            [1] = "Skill_Punch",
                            [2] = "Left"
                        }
                    }
    
                    Remote:FireServer(unpack(Arguments))
                    game:GetService("RunService").RenderStepped:wait()
                    local Remote = game.ReplicatedStorage['RemoteEvent']
    
                    local Arguments = {
                        [1] = {
                            [1] = "Skill_BulletPunch",
                            [2] = "Right",
                            [3] = Name.position
                        }
                    }
    
                    Remote:FireServer(unpack(Arguments))
                    game:GetService("RunService").RenderStepped:wait()
                    local Remote = game.ReplicatedStorage['RemoteEvent']
    
                    local Arguments = {
                        [1] = {
                            [1] = "Skill_BulletPunch",
                            [2] = "Left",
                            [3] = Name.position
                        }
                    }
    
                    Remote:FireServer(unpack(Arguments))
                    game:GetService("RunService").RenderStepped:wait()
                      local Remote = game.ReplicatedStorage['RemoteEvent']
                    local Arguments = {
                        [1] = {
                            [1] = "Skill_SpherePunch",
                            [2] = Vector3.new(0,99999999999,0)
